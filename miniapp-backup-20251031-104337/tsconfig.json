{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "resolveJsonModule": true,
    "declaration": false,
    "sourceMap": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "noEmitOnError": false,
    "allowJs": true,
    "checkJs": false,
    "types": ["miniprogram-api-typings"]
  },
  "include": ["miniprogram/**/*", "typings/**/*"],
  "exclude": ["node_modules", "miniprogram/**/*.js"]
}
